   "source": [
    "# Slow estimation of pi\n",
    "\n",
    "import random\n",
    "\n",
    "def estimate_pi(n):\n",
    "    m = 0\n",
    "    for i in range(n):\n",
    "        x = random.uniform(0, 1)\n",
    "        y = random.uniform(0, 1)\n",
    "        if x*x + y*y <= 1:\n",
    "            m += 1\n",
    "    return 4 * m / n\n",
    "\n",
    "estimate_pi(1000000000)"
   ]
  }
 ],
